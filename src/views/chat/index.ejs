<!DOCTYPE html>
<html>
  <head>
    <title>Webchat</title>
    <link rel="stylesheet" href="/css/chat.css">
  </head>
  <body>
    <form action="" class="nickBox">
      <input
        class="nickInput"
        autocomplete="off"
        placeholder="Insira seu nickname"
        data-testid="nickname-box"
      /><button class="nickButton" data-testid="nickname-button">Salvar</button>
    </form>
    <ul class="users"></ul>
    <ul class="messages"></ul>
    <form action="" class="messageBox">
      <input
        class="messageInput"
        autocomplete="off"
        data-testid="message-box"
      /><button class="sendButton" data-testid="send-button">Enviar</button>
    </form>
    <script src="/socket.io/socket.io.js"></script>
    <script src='./js/chat.js'></script>
  </body>
</html>