<!DOCTYPE html>
<html lang="en">
<head>
  <title>Web Chat</title>
</head>
<body>
  <form>
    <input type="text" placeholder="NickName" id="nickname-box" data-testid="nickname-box" />
    <button id="changeNickName" data-testid="nickname-button">Chenge Nickname</button>
  </form>
  <ul id="onlineUsers">
    <li id="nickName" data-testid="online-user">NickName</li>
  </ul>
  <ul id="messages" data-testid="message-box">
  </ul>
  <form>
    <input type="text" id="message-box" data-testid="message-box" />
    <button data-testid="send-button" id="send-button">Send Message</button>
  </form>
  <script src="/socket.io/socket.io.js"></script>
  <script src="/chat.js"></script>
</body>
</html>