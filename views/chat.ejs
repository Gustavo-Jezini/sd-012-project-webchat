<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="http://localhost:3000/socket.io/socket.io.js"></script>
    <title>Webchat</title>
  </head>
  <body>
    <header>
      <form>
        <h3 id="userNickname" data-testid="online-user"></h3>
        <input data-testid="nickname-box"/>
        <button data-testid="nickname-button">Salvar</button>
      </form>
    </header>
    <div>
      <ul id="messages">

      </ul>
      <form id="messageForm">
        <input id="messageBox" data-testid="message-box"/>
        <button id="sendButton" data-testid="send-button">
          Enviar
        </button>
      </form>
    </div>
    <script src="./js/chat.js"></script>
  </body>
</html>